plugin.tx_indiznews_news {
    view {
        templateRootPaths.0 = EXT:indiz_news/Resources/Private/Templates/
        templateRootPaths.1 = {$plugin.tx_indiznews_news.view.templateRootPath}
        partialRootPaths.0 = EXT:indiz_news/Resources/Private/Partials/
        partialRootPaths.1 = {$plugin.tx_indiznews_news.view.partialRootPath}
        layoutRootPaths.0 = EXT:indiz_news/Resources/Private/Layouts/
        layoutRootPaths.1 = {$plugin.tx_indiznews_news.view.layoutRootPath}
    }
    persistence {
        storagePid = {$plugin.tx_indiznews_news.persistence.storagePid}
        #recursive = 1
    }
    features {
        #skipDefaultArguments = 1
        # if set to 1, the enable fields are ignored in BE context
        ignoreAllEnableFieldsInBe = 0
    }
    mvc {
        #callDefaultActionIfActionCantBeResolved = 1
    }
}